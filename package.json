  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yesmeck/formsy-react-inputs.git"
  },
  "keywords": [
    "react",
    "formsy-react",
    "react-form",
    "react-component"
  ],
  "author": "Wei Zhu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yesmeck/formsy-react-inputs/issues"
  },
  "homepage": "https://github.com/yesmeck/formsy-react-inputs#readme",
  "peerDependencies": {
    "formsy-react": "0.14.x"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "jest-cli": "^0.5.8",
    "react": "^0.13.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "react-mixin": "^3.0.0"

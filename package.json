{
  "name": "formsy-react-inputs",
  "version": "1.0.0",
  "description": "A set of formsy-react inputs without any fancy.",
  "main": "./release/formsy-react-inputs.js",
  "scripts": {
    "start": "webpack-dev-server --content-base build",
    "release": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
